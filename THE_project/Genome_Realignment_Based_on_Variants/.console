bwa index data/test.fa

bwa mem data/test.fa data/test.fastq > data/test.sam

samtools sort data/test.sam -o data/test.sam --output-fmt SAM

gcc *.c -o main -lhts

./main --faFile data/test.fa --fastqFile data/test.fastq --samFile data/test.sam --vcfFile data/test.vcf --outputFile data/grbvOut.txt --integrateVcfToSam