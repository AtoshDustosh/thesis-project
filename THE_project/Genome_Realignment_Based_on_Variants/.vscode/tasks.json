{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc-9 build active file",
      "command": "/usr/bin/gcc-9",
      "args": [
        "-g",
        "*.c",
        "-o",
        "${fileDirname}/main",
        "-lhts"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Build configuration"
    }
  ],
  "version": "2.0.0"
}